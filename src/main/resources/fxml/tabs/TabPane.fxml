<AnchorPane id="tabbedDisplayAnchor" maxHeight="-1.0" maxWidth="-1.0" 
            prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" 
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" 
            fx:controller="com.trend.mooringcalculator.controllers.TabPaneManger">
    <children>
        <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <fx:include fx:id="bargeTab" source="BargeTab.fxml" />
                <fx:include fx:id="cargoTab" source="CargoEnvTab.fxml" />
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
